{
  "name": "tod_list",
  "version": "1.0.0",
  "description": "todo list app",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:server": "tsc",
    "build:client": "vite build",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:client": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "test": "npm run test"
  },
  "author": "Elver Rodriguez",
  "license": "ISC",
  "dependencies": {
    "express": "^5.2.1",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "vite": "^5.0.11",
    "concurrently": "^8.2.2"
  }
}