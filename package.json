{
  "name": "tod_list",
  "version": "1.0.0",
  "description": "todo list app",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:server": "tsc",
    "build:client": "vite build",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:client": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "test": "jest",
    "test:api": "jest --testPathPattern=tests/api",
    "test:frontend": "jest --testPathPattern=tests/frontend",
    "test:selenium": "ts-node tests/e2e/todo.e2e.ts",
    "test:e2e": "npm run test:selenium",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Elver Rodriguez",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "sqlite3": "^5.1.6",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/selenium-webdriver": "^4.1.9",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.3",
    "@vue/vue3-jest": "^29.2.6",
    "chromedriver": "^120.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "selenium-webdriver": "^4.16.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}